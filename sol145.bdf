SOL 145
TIME 600
CEND
TITLE=WINDY - LITE FWT
ECHO=NONE
MAXLINES=999999999
AECONFIG=AeroSG2D
AESYMXZ=Symmetric
AESYMXY=Asymmetric
AEROF=ALL
APRES=ALL
DISPLACEMENT(SORT1,REAL)=ALL
SPCFORCES(SORT1,REAL)=ALL
STRESS(SORT1,REAL,VONMISES,BILIN)=ALL
SUBCASE 1
  SUBTITLE=Flutter
  SPC=3
  FMETHOD = 4
  METHOD = 10
  LOAD=11
BEGIN BULK
PARAM,POST,0
PARAM,WTMASS,1.
PARAM,SNORM,20.
PARAM,PRTMAXIM,YES
MDLPRM,HDF5,0
